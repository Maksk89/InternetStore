<div class="good">
    <div class="good-pic" style="background-image:url('{#baseUrl}{#pic}')">
        <a href="{#baseUrl}good/{#id}">
            <div class="good-info">
                <h3>{#name}</h3>
                <p>{#text}</p>
                <p>{#price}</p>
            </div>
        </a>
    </div>
</div>